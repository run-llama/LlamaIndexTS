    "@turbo/gen": "^1.10.16",
    "@types/jest": "^29.5.10",
    "eslint": "^8.54.0",
    "eslint-config-custom": "workspace:*",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "ts-jest": "^29.1.1",
    "turbo": "^1.10.16"
  },
  "packageManager": "pnpm@8.10.5+sha256.a4bd9bb7b48214bbfcd95f264bd75bb70d100e5d4b58808f5cd6ab40c6ac21c5",
  "pnpm": {
    "overrides": {
      "trim": "1.0.1",
      "@babel/traverse": "7.23.2"
