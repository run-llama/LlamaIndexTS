{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\"",
    "lint": "turbo run lint",
    "prepare": "husky install",
