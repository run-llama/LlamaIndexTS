{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist/type",
    "tsBuildInfoFile": "./dist/.tsbuildinfo",
    "emitDeclarationOnly": true,
    "module": "node16",
    "moduleResolution": "node16",
    "skipLibCheck": true,
    "strict": true,
    "paths": {
      "@llamaindex/env": ["../env/src/index.ts"],
      "@llamaindex/env/*": ["../env/src/*.ts"]
    }
  },
  "include": ["./src"],
  "exclude": ["node_modules"],
  "references": [
    {
      "path": "../env/tsconfig.json"
    }
  ]
}
