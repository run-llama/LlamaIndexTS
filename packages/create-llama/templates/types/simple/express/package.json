{
  "name": "llama-index-express",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsup index.ts --format esm --dts",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsup index.ts --format esm --dts --watch\" \"nodemon -q dist/index.js\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "llamaindex": "0.0.37"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.5",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.1",
    "tsup": "^7.3.0",
    "typescript": "^5.3.2"
  }
}
