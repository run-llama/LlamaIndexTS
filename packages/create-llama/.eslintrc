{
  "root": false,
  "rules": {
    "turbo/no-undeclared-env-vars": [
      "error",
      {
        "allowList": [
          "OPENAI_API_KEY",
          "npm_config_user_agent",
          "http_proxy",
          "https_proxy",
          "MODEL",
          "NEXT_PUBLIC_CHAT_API",
          "NEXT_PUBLIC_MODEL"
        ]
      }
    ]
  }
}